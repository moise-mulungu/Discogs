<h1>New Genre</h1>

<%= form_with model: @genre do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
    <% @genre.errors.full_messages_for(:name).each do |message| %>
        <div><%= message %></div>
      <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>